<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ControllerViewAllPersons">
    <children>
        <TableView fx:id="tableView" editable="true" prefHeight="400.0" prefWidth="600.0" tableMenuButtonVisible="true">
            <columns>
                <TableColumn fx:id="columnID" onEditCancel="#doCancel" onEditCommit="#doCommit" onEditStart="#doStart" prefWidth="99.0" text="ID" />
                <TableColumn fx:id="columnName" onEditCancel="#doCancel" onEditCommit="#doCommit" onEditStart="#doStart" prefWidth="74.0" text="Name" />
                <TableColumn prefWidth="140.0" text="Lastname" />
                <TableColumn prefWidth="112.0" text="Job" />
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
        </TableView>
    </children>
</AnchorPane>
